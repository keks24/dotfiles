[include]
    path = ~/.gitconfig.d/gitalias.txt
[alias]
    call = commit --all
    d = diff --color-words
    dp = checkout --patch
    lft = lfs track
    lfmt = lfs migrate info --everything --fixup
    lfmi = lfs migrate import --fixup
    # git config lfs.https://<some_fqdn>/keks24/<some_repository_name>/info/lfs.locksverify true
    rp = reset --patch
#[pull]
#    rebase = true
[commit]
    gpgsign = true
[filter "lfs"]
    process = git-lfs filter-process
    required = true
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
